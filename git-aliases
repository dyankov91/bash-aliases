# Git
alias gs='git status'
alias ga='git add'
alias gaa='git add .'
alias gc='git commit -m'
alias gca='git commit --amend'
alias gp='git push'
alias gf='git fetch'
alias gpl='git pull'
alias gl='git log'
alias gr='git checkout --'
alias gra='git checkout -- .'
alias gco='git checkout'
alias gm='git merge'
alias gd='git diff'
alias gdf='git diff --name-only'
